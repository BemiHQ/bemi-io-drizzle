  "bugs": {
    "url": "https://github.com/BemiHQ/bemi-io-drizzle/issues"
  },
  "homepage": "https://github.com/BemiHQ/bemi-io-drizzle",
  "dependencies": {
    "commander": "^11.1.0"
  },
  "peerDependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.4",
    "ts-node": "^10.9.1",
    "tsup": "^8.0.1",
    "typescript": "^5.0.0"
  },
  "registry-url": "https://registry.npmjs.org/",
  "scripts": {
